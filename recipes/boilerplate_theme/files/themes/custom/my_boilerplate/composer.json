{
  "name": "drupal/my_boilerplate",
  "type": "drupal-custom-theme",
  "version": "dev-main",
  "description": "My custom boilerplate theme for Drupal.",
  "require": {
    "drupal/core": "^11"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\my_boilerplate\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  }
}
